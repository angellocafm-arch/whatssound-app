{
  "buildCommand": "npx expo export --platform web",
  "outputDirectory": "dist",
  "framework": null,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3"
    },
    "api/*.ts": {
      "runtime": "@vercel/node@3"
    }
  },
  "rewrites": [
    { "source": "/((?!api|_expo|assets|favicon\\.ico).*)", "destination": "/index.html" }
  ],
  "crons": [
    {
      "path": "/api/regenerate-boosts",
      "schedule": "0 11 * * 5"
    }
  ]
}
